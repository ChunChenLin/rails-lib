<div class="page-header">
  <%= link_to new_maintain_state_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Maintain state
  <% end %>
  <h1>Listing maintain_states</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th>State</th>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @maintain_states) do |maintain_state| %>
            <td><%= maintain_state.state %></td>
            <td><%= link_to 'Show', maintain_state %></td>
        <td><%= link_to 'Edit', edit_maintain_state_path(maintain_state) %></td>
        <td><%= link_to 'Destroy', maintain_state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
